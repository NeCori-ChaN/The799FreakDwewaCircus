# game/script.rpy:133
translate chinese nomes_66334b1c:

    # narrator "{color=#8faa7f}You put yourself in front of the man."
    narrator "{color=#8faa7f}你迅速上前，挡在他面前。"

# game/script.rpy:134
translate chinese nomes_dde1674c:

    # mc "That’s enough! There’s no need to get violent! He’s just doing his job!"
    mc "够了！别无理取闹了，他不过是在做自己的工作，又没招惹谁！"

# game/script.rpy:135
translate chinese nomes_cebe62a5:


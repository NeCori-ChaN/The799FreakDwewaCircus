# game/script.rpy:126
translate chinese nomes_a2c05c64:

    # d "I’m talking to you, you freak! Get out of our town!!"
    d "我说的就是你，怪物！赶紧从我们镇上滚出去！！"

# game/script.rpy:128
translate chinese nomes_7f1882d4:

    # mc "{color=#a0b0c0}(Okay, that guy’s really crossing the line now!!)"
